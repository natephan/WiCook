        app:layout_constraintBottom_toBottomOf="@+id/view" app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintVertical_bias="0.805" android:textAllCaps="false" android:textSize="40sp"
        android:background="#FFFFA1" android:textColor="#C93737"/>
    <Button
        android:text="Vegan"
        android:layout_width="314dp"
        android:layout_height="114dp" android:id="@+id/Vegan"
