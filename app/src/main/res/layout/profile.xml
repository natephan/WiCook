<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".login" android:background="#FFA700">
    <Button
            android:text="My Friends"
            android:layout_width="314dp"
            android:layout_height="114dp" android:id="@+id/friends_button"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/view" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintVertical_bias="0.541" android:textAllCaps="false" android:textSize="40sp"
            android:background="#FFFFA1" android:textColor="#C93737"/>
    <Button
            android:text="Account"
            android:layout_width="314dp"
            android:layout_height="114dp" android:id="@+id/button6"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/view" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintVertical_bias="0.784" android:textAllCaps="false" android:textSize="40sp"
            android:background="#FFFFA1" android:textColor="#C93737"/>
    <View
            android:layout_width="match_parent"
            android:layout_height="88dp" android:id="@+id/view"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="1.0" android:background="#FFFFA1"/>
    <TextView
            android:text="Social"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView8"
            android:textSize="46sp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/view" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintVertical_bias="0.023"
            android:textColor="#C93737"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/settings" android:id="@+id/settings_button"
            android:background="#00FFFFFF"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="@+id/view"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.955" app:layout_constraintVertical_bias="0.023"/>
    <ImageButton
            android:layout_width="100dp"
            android:layout_height="88dp" app:srcCompat="@drawable/notification"
            android:id="@+id/notif_nav"
            android:background="#00FFFFFF"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="1.0"/>
    <ImageButton
            android:layout_width="104dp"
            android:layout_height="87dp" app:srcCompat="@drawable/homeorange" android:id="@+id/home_nav"
            android:background="#00FFFFFF"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.998"/>
    <ImageButton
            android:layout_width="100dp"
            android:layout_height="88dp" app:srcCompat="@drawable/cookbookicon"
            android:id="@+id/cookbook_nav"
            android:background="#00FFFFFF"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.334"
            app:layout_constraintVertical_bias="1.0" android:layout_marginLeft="5dp" android:layout_marginStart="5dp"/>
    <ImageButton
            android:layout_width="100dp"
            android:layout_height="88dp" app:srcCompat="@drawable/exploreicon"
            android:id="@+id/explore_nav"
            android:background="#00FFFFFF"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.678"
            app:layout_constraintVertical_bias="1.0" android:layout_marginRight="6dp" android:layout_marginEnd="6dp"/>
    <View
            android:layout_width="209dp"
            android:layout_height="72dp" android:id="@+id/view2"
            android:background="#000000"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.109"
            android:layout_marginTop="2dp"/>
    <TextView
            android:text="Feed"
            android:layout_width="203dp"
            android:layout_height="68dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/view" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.116" android:textColor="#C93737" android:textSize="40dp"
            android:clickable="true" android:gravity="center" android:background="#FFFFA1"
            android:id="@+id/feed_button" android:layout_marginLeft="1dp" android:layout_marginStart="1dp"
            android:layout_marginBottom="2dp"/>
    <TextView
            android:text="Profile"
            android:layout_width="205dp"
            android:layout_height="68dp" android:id="@+id/textView20"
            android:background="#FFFF00" android:textSize="40sp"
            android:gravity="center" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/view" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.116"/>
    <Button
            android:text="MyCookbook"
            android:layout_width="314dp"
            android:layout_height="114dp" android:id="@+id/cookbook_button"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/view" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintVertical_bias="0.298" android:textAllCaps="false" android:textSize="40sp"
            android:background="#FFFFA1" android:textColor="#C93737"/>

</androidx.constraintlayout.widget.ConstraintLayout>